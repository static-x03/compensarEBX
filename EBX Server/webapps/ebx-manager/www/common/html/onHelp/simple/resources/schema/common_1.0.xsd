	</xs:simpleType>
	<xs:simpleType name="color">
		<xs:restriction base="xs:string">
			<xs:length value="7"/>
			<xs:pattern value="#[A-Fa-f0-9]{6}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="dataspaceKey">
		<xs:restriction base="xs:string" >
			<xs:maxLength value="33"/>
			<xs:pattern value="[BV][a-zA-Z0-9_:.\\-\\|]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="datasetName">
		<xs:restriction base="xs:string" >
			<xs:maxLength value="64"/>
			<xs:pattern value="[a-zA-Z_][-a-zA-Z0-9_.]*"/>
		</xs:restriction>
	</xs:simpleType>
	<!--  complex type for UDA -->
	<xs:complexType name="UDA">
		<xs:annotation>
			<xs:documentation xml:lang="fr-FR">
				Attribut(s) défini(s) par l'utilisateur
			</xs:documentation>
			<xs:documentation xml:lang="en-US">
				User defined attribute(s)
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="attribute" type="xs:string">
				<xs:annotation>
					<xs:appinfo>
						<osd:otherFacets>
							<osd:tableRef>
								<tablePath>//udaCatalog</tablePath>
							</osd:tableRef>
						</osd:otherFacets>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="value" minOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="xsString" minOccurs="0"
							type="xs:string" />
						<xs:element name="xsBoolean" minOccurs="0"
							type="xs:boolean" />
						<xs:element name="xsInt" minOccurs="0"
							type="xs:int" />
						<xs:element name="xsDecimal" minOccurs="0"
							type="xs:decimal" />
						<xs:element name="xsDateTime" minOccurs="0"
							type="xs:dateTime" />
						<xs:element name="xsDate" minOccurs="0"
							type="xs:date" />
						<xs:element name="xsTime" minOccurs="0"
							type="xs:time" />
						<xs:element name="xsName" minOccurs="0"
							type="xs:Name" />
						<xs:element name="xsAnyURI" minOccurs="0"
							type="xs:anyURI" />
						<xs:element name="osdText" minOccurs="0"
							type="osd:text" />
						<xs:element name="osdHtml" minOccurs="0"
							type="osd:html" />
						<xs:element name="osdEmail" minOccurs="0"
							type="osd:email" />
						<xs:element name="osdPassword" minOccurs="0"
							type="osd:password" />
						<xs:element name="osdLocale" minOccurs="0"
							type="osd:locale" />
						<xs:element name="osdColor" minOccurs="0"
							type="osd:color" />
						<xs:element name="osdDataspaceKey" minOccurs="0"
							type="osd:dataspaceKey" />
						<xs:element name="osdDatasetName" minOccurs="0"
							type="osd:datasetName" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<!--  complex type for UDA Catalog -->
	<xs:complexType name="UDACatalog">
		<xs:annotation>
			<xs:documentation xml:lang="fr-FR">
				Catalogue des attributs. Catalogue des attributs définis
				par l'utilisateur.
			</xs:documentation>
			<xs:documentation xml:lang="en-US">
				Attributes catalog. Catalog for user defined attributes.
			</xs:documentation>
			<xs:appinfo>
				<osd:table>
					<primaryKeys>/name</primaryKeys>
				</osd:table>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="xs:Name" />
			<xs:element name="type">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="xs:string" />
						<xs:enumeration value="xs:boolean" />
						<xs:enumeration value="xs:int" />
						<xs:enumeration value="xs:decimal" />
						<xs:enumeration value="xs:dateTime" />
						<xs:enumeration value="xs:date" />
						<xs:enumeration value="xs:time" />
						<xs:enumeration value="xs:Name" />
						<xs:enumeration value="xs:anyURI" />
						<xs:enumeration value="osd:text" />
						<xs:enumeration value="osd:html" />
						<xs:enumeration value="osd:email" />
						<xs:enumeration value="osd:password" />
						<xs:enumeration value="osd:locale" />
						<xs:enumeration value="osd:color" />
						<xs:enumeration value="osd:dataspaceKey" />
						<xs:enumeration value="osd:datasetName" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="documentation" osd:access="RW">
				<xs:annotation>
					<xs:documentation xml:lang="fr-FR">
						Libellé et description. Libellés et descriptions
						internationalisés, ils sont affichés à
						l'utilisateur de EBX®.
					</xs:documentation>
					<xs:documentation xml:lang="en-US">
						Label and description. Internationalized labels
						and descriptions, they are displayed to end user
						in EBX®.
					</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:annotation>
						<xs:documentation xml:lang="fr-FR">
							Libellé et description
						</xs:documentation>
						<xs:documentation xml:lang="en-US">
							Label and description
						</xs:documentation>
					</xs:annotation>
					<xs:sequence>
						<xs:element name="localizedDocumentations"
							minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="fr-FR">
									[locale]
								</xs:documentation>
								<xs:documentation xml:lang="en-US">
									[locale]
								</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="locale"
										type="osd:locale">
										<xs:annotation>
											<xs:documentation
												xml:lang="fr-FR">
												Région
											</xs:documentation>
											<xs:documentation
												xml:lang="en-US">
												Locale
											</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="label"
										type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation
												xml:lang="fr-FR">
												Libellé
											</xs:documentation>
											<xs:documentation
												xml:lang="en-US">
												Label
											</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="description"
										minOccurs="0">
										<xs:annotation>
											<xs:documentation
												xml:lang="fr-FR">
												Description
											</xs:documentation>
											<xs:documentation
												xml:lang="en-US">
												Description
											</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction
												base="xs:string">
												<xs:maxLength
													value="99999" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
