<?xml version="1.0" encoding="UTF-8"?>
<root>
    <definition>
        <process>
            <moduleName>compensar-mdm</moduleName>
            <dataContext>
                <variable>
                    <name>dataSet</name>
                    <inputParameter>true</inputParameter>
                    <outputParameter>false</outputParameter>
